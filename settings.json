{
  "apiKeyHelper": "/opt/dev/bin/user/devx llm-gateway print-token --key",
  "env": {
    "ANTHROPIC_BASE_URL": "https://proxy.shopify.ai/vendors/anthropic-claude-code",
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": []
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "vault-mcp",
    "figma-mcp",
    "gworkspace-mcp"
  ],
  "hooks": {
    "PreToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.claude/agent-sims-hook.sh working"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.claude/agent-sims-hook.sh thinking"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.claude/agent-sims-hook.sh offline"
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.quick-ping/quick-ping.sh",
            "async": true,
            "timeout": 5
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.claude/notify.sh"
          }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.claude/notify.sh"
          }
        ]
      },
      {
        "matcher": "permission_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.quick-ping/quick-ping.sh",
            "async": true,
            "timeout": 5
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tobybarnes_shop/.quick-ping/quick-ping.sh",
            "async": true,
            "timeout": 5
          }
        ]
      }
    ]
  }
}
